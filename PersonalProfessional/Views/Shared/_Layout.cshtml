<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - Michael Nhu Shoemake</title>
    @*<title>@ViewData["Title"] - PersonalProfessional</title>*@
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/css/site.css" />
    <link rel="shortcut icon" href="~/images/icons/mns_small2.png" />
</head>
<body>
    <header>
        <nav class="navbar navbar-expand-sm navbar-toggleable-sm navbar-light bg-white border-bottom mb-auto">
            <div class="container">
                <a class="navbar-brand" asp-area="" asp-controller="Home" asp-action="Index">Michael Nhu Shoemake</a>
                <button class="navbar-toggler" type="button" data-toggle="collapse" data-target=".navbar-collapse" aria-controls="navbarSupportedContent"
                        aria-expanded="false" aria-label="Toggle navigation">
                    <span class="navbar-toggler-icon"></span>
                </button>
                <div class="navbar-collapse collapse d-sm-inline-flex flex-sm-row">
                    <ul class="navbar-nav mr-auto">
                        <li class="nav-item">
                            <a class="nav-link" asp-area="" asp-controller="Home" asp-action="Index">Home</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link" asp-area="" asp-controller="Home" asp-action="Skills">Skills</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link" asp-area="" asp-controller="Home" asp-action="Projects">Projects</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link" asp-area="" asp-controller="Home" asp-action="Bio">Bio</a>
                        </li>
                    </ul>
                </div>
            </div>
            <nav id="soc_media_icons" class="nav navbar-nav navbar-right navbar-expand-sm">
                <ul class="nav navbar-nav navbar-right navbar-expand-sm">
                    <li class="nav-item">
                        <a href="https://www.linkedin.com/in/michael-nhu-shoemake-0b3a8679/">
                            <img src="~/images/icons/header/linked_icon_gray.png" height="20" width="20" alt="LinkedIn Profile" />
                        </a>
                    </li>
                    <li class="nav-item">
                        <a href="https://www.github.com/mnshoemake/">
                            <img src="~/images/icons/header/github_icon_gray.svg" height="20" width="20" alt="GitHub Profile" />
                        </a>
                    </li>
                    <li class="nav-item">
                        <a href="https://twitter.com/mnshoemake">
                            <img src="~/images/icons/header/twitter_icon_gray.png" alt="Twitter Profile" height="20" width="20" />
                        </a>
                    </li>
                </ul>
            </nav>
        </nav>
    </header>
    <hr class="header_rule">
    <div class="container">
        <main role="main" class="pb-3">
            @RenderBody()
        </main>
    </div>
    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
    @RenderSection("Scripts", required: false)
</body>
</html>
